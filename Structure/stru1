#include<stdio.h>
struct mark
{
    char name[20];
    int roll,marks;
    
};
int main()
{
    struct mark s[6];
    for(int i=0;i<6;i++)
    {
    scanf("%s%d%d",s[i].name,&s[i].roll,&s[i].marks);
    }
    for(int i=1;i<=6;i++)
    {
        if((s[i].marks/4)>=0.95)
         printf("\n%s eligible",s[i].name);
    }
}
